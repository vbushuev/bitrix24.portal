// main: main.styl, out: forms.css
.form
    input,select,button
        height 2em
        line-height 2em
        color rgba(0,0,0,.8)
        border rgba(0,0,0,.5) 1px solid
        border-radius .4em
        font-size 1em
        width 12em
        padding 0 .4em
    button
        width 6em
        height 2em
        line-height 2em
        border none
        color rgba(0,0,0,.8)
        background-color rgb(148,185,220)
        cursor pointer
